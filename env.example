# 系统配置文件
# 复制此文件为 .env 并填入你的实际配置

# 是否使用临时对话
IS_TEMP_CONVERSATION=true

# 是否使用自定义 SSO 令牌模式
IS_CUSTOM_SSO=false

# API 密钥
API_KEY=your_api_key_here

# PICGO 图床密钥（用于图像生成的流式输出）
PICGO_KEY=your_picgo_key_here

# TUMY 图床密钥（用于图像生成的流式输出，与 PICGO 二选一）
TUMY_KEY=your_tumy_key_here

# 代理设置（可选）
PROXY=http://127.0.0.1:7890

# 管理员功能开关
MANAGER_SWITCH=false

# 管理员密码
ADMINPASSWORD=admin123

# Cloudflare Clearance（用于绕过防护）
CF_CLEARANCE=your_cloudflare_clearance_here

# 服务器端口
PORT=5200

# 是否显示思考过程
SHOW_THINKING=true

# 是否显示搜索结果
ISSHOW_SEARCH_RESULTS=true

# SSO Cookie 令牌（多个用分号分隔）
SSO=ssoCookie1;ssoCookie2;ssoCookie3 